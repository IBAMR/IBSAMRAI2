SAMRAI	      =	@top_srcdir@
SRCDIR	      =	@srcdir@
SUBDIR        = source/patchdata/templates/automaticNond
VPATH	      =	@srcdir@
OBJECT        = ../../../..

default: library

include $(OBJECT)/config/Makefile.config
include $(SRCDIR)/Makefile.objects

library:	lib1d lib2d lib3d

lib1d:
	$(MAKE) NDIM=1 libXd

lib2d:
	$(MAKE) NDIM=2 libXd

lib3d:
	$(MAKE) NDIM=3 libXd

libXd:	$(SAMRAI_LIBRARY_TARGETS)

clean:
	$(SAMCLEAN)

include $(SRCDIR)/Makefile.depend
